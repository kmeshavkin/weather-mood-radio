{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["airbnb", "plugin:prettier/recommended"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "require-jsdoc": 0,
        "no-plusplus": 0,
        "no-console": 0,
        "react/prop-types": 0,
        "prettier/prettier": "error",
        "react/jsx-curly-newline": 0
    }
}